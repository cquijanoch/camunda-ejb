<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.unsa.mybatis.mapper.UsuarioMapper">

	<insert id="insert" parameterType="org.unsa.mybatis.bean.Usuario"  statementType="PREPARED"  useGeneratedKeys="true" keyProperty="usuarioId" keyColumn="id_usuario" >
		INSERT INTO usuario (nombre, dni)
		values (#{nombre}, #{dnistring})
	</insert>
	
